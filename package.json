{
  "name": "somiibo",
  "version": "1.0.10",
  "description": "Social media automation bot for Instagram, Facebook, Twitter, SoundCloud, LinkedIn, Pinterest, Google+, Spotify, Tumblr, and more.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npm run prepare && ./node_modules/mocha/bin/mocha test/ --recursive --timeout=10000",
    "prepare": "node -e \"require('prepare-package')()\"",
    "prepare:watch": "nodemon -w ./src -e '*' --exec 'npm run prepare'"
  },
  "engines": {
    "node": ">=12"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/somiibo/somiibo"
  },
  "keywords": [
    "somiibo"
  ],
  "author": "ITW Creative Works",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/somiibo/somiibo/issues"
  },
  "homepage": "https://somiibo.com",
  "preparePackage": {
    "input": "./src",
    "output": "./dist",
    "replace": {}
  },
  "dependencies": {
    "itwcw-package-analytics": "^1.0.4"
  },
  "devDependencies": {
    "mocha": "^10.4.0",
    "prepare-package": "^1.1.12"
  }
}